<launch>
  <arg name="x" default="0.0"/>
  <arg name="y" default="0.0"/>
  <arg name="z" default="0.20"/>
  <arg name="yaw" default="0.0"/>

  <!--<group ns="$(arg robot_namespace)">    -->

  <include file="$(find leo_control)/launch/control.launch">
	</include>

	<!--<include file="$(find leo_description)/launch/description.launch" >
  	</include>-->

	<node name="spawn_leo_model" pkg="gazebo_ros" type="spawn_model"
	args="
    -x $(arg x)
    -y $(arg y)
    -z $(arg z)
    -Y $(arg yaw)
    -unpause
    -param robot_description 
    -urdf 
    -model leo" />

  <!--</group>-->

</launch>
